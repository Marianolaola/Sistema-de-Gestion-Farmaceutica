<Window x:Class="Sistema_de_Gestión_Farmacéutica.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sistema_de_Gestión_Farmacéutica"
        Title="Sistema de Gestión Farmacéutica"
        WindowStartupLocation="CenterScreen"
        WindowState="Normal">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>

        <StackPanel Background="DodgerBlue" Grid.Column="0" VerticalAlignment="Stretch">
            <TextBlock Text="Inventario" Foreground="White" FontSize="18" FontWeight="Bold" Margin="10"/>
            <Separator Margin="0,0,0,5"/>
            
        </StackPanel>
    </Grid>
</Window>
